package level0;

import java.util.Arrays;

public class d241030_두정수사이의합 {
	public static void main(String[] args) {
		int a = 5;
		int b = 3;
		long answer =0;
		
		int[] arr = new int[2];
		arr[0] = a;
		arr[1] = b;
		
		Arrays.sort(arr);
		
		System.out.println(Arrays.toString(arr));
		
        for(int i = a; i <=b ; i++){
            if( a<=b){
            answer += a;
            }
        }        
	}
}
