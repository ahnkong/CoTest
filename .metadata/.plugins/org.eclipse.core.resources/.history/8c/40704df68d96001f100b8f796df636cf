package level0;

import java.util.Arrays;
import java.util.Iterator;

public class d241030_두정수사이의합 {
	public static void main(String[] args) {
		int a = 5;
		int b = 3;
		long answer =0;
		long temp =0;
		
		temp = Math.abs(a-b);
		for (int i = 1; i < temp; i++) {
			System.out.println(temp);
			if( a>b) {
				answer += a-i;
			} else {
				answer += a+i;
			}
			System.out.println(answer);
        }    
	}
}
