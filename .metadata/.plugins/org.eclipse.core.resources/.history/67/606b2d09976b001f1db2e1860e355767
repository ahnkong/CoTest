import java.util.HashSet;
import java.util.Set;

package level1;

//3) 클래스로 푸는 방법
/*
	 1- 데이터 세팅
	 2- 게터와 세터 작성
	 3- 로직 구현
 * */
public class 폰켓몬class구현 {
	public static void main(String[] args) {
		//test case1
        int[] nums1 = {3, 1, 2, 3};
        PoketmonChoice sel1 = new PoketmonChoice(nums1);
        System.out.println(sel1.getTypes()); // 결과는 2
	}


}	

class PoketmonChoice {
	private int [] nums;
	private int types;

	
	//생성자
	public PoketmonChoice(int[] nums, int types) {
		super();
		this.nums = nums;
		this.types = types;
		
	}
		
	//게터 세터
	public int[] getNums() {
		return nums;
	}


	public void setNums(int[] nums) {
		this.nums = nums;
	}


	public int getTypes() {
		return types;
	}


	public void setTypes(int types) {
		this.types = types;
	}
	
	
}