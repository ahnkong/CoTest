package level1;

public class d241001_약수의합 {
	public static void main(String[] args) {
		int n = 12;
		int answer = 0;
        int temp = 0;      
     
        //삼항연산자로 가능??
//        for( int i = 1; i <= n ; i++){
//            if ( n%i == 0){
//                temp += i;
//            }
//        }
        for( int i = 1; i <= n ; i++){
        	temp += (n % i == 0) ? i : 0 ;
        }
        
        
 
        System.out.println(temp);
		
	}
}
