package level1;


// 결국 아스키 코드 문제 
public class d241011_시저암호 {
	public static void main(String[] args) {
		
		
		String s = "AB";
		int n = 1;
		
		char str = "";
		
		String answer = "";
		
		for (int i = 0; i < s.length(); i++) {
			str += s.charAt(i)+1;
			System.out.println(str+ " ");
			System.out.println((int)str);
			
		}
	}
}
