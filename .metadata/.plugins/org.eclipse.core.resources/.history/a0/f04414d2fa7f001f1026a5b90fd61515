package level1;

public class d241001_약수의합 {
	public static void main(String[] args) {
		int n = 12;
		int answer = 0;
        int temp = 0;      
     
        
        for( int i = 0; i <= n ; i++){
            if ( n%i != 0){
                temp += i;
            }
        }
        System.out.println(temp);
		
	}
}
