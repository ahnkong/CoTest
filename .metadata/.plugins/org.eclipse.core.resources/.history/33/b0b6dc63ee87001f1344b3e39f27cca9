package practiceCode;

public class ascii {

	public static void main(String[] args) {
		
		char ch = 'A';

		int asciiCh = (int)ch;
		
		
		System.out.println("ch형 : " + ch);
		System.out.println("ascill형 : " + asciiCh);
		System.out.println("ascill 하드 코딩 : " + (int)ch);
		System.out.println("ascill 하드 코딩 : " + (int)'A');
		
		
		
		
		int num = 77;
		char asciiCh2 = (char)num;
		
		System.out.println("num형 : " + num);
		System.out.println("ascii형 : " + asciiCh2);
		System.out.println("ascii형 : " + (char)num);
		System.out.println("ascii형 : " + (char)77);
		
		
	}
}
