package prac;

public class condition {
	public static void main(String[] args) {
		
		int myAge = 19;
		
		if(myAge == 19) {
			System.out.println("19살 입니다.");
		}
		if(myAge != 19) {
			System.out.println("19살이 아닙니다.");
		}
		if(myAge > 19) {
			System.out.println("성인입니다.");
		}
		if(myAge <= 19) {
			System.out.println("학생입니다.");
		}
		System.out.println(myAge);
	}
}
