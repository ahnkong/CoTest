package level1;

public class 폰켓몬class구현 {
	//3) 클래스로 푸는 방법
	/*
	 1- 데이터 세팅
	 2- 게터와 세터 작성
	 3- 로직 구현
	 * */
	
	private int number; // 
	private int count; //
	
	//생성자
	public 폰켓몬class구현(int number, int count) {
		super();
		this.number = number;
		this.count = count;
	}
	
	
	// 게터 세터
	//번호 가져오기
	public int getNumber() {
		return number;
	}

	
	//
	public void setNumber(int number) {
		this.number = number;
	}
	
	
	// 포켓몬 종류 갯수 증가시키기
	public int getCount() {
		return count;
	}

	//포켓몬 갯수 설정하기
	public void setCount(int count) {
		this.count = count;
	}
	
	//
	
	
}
