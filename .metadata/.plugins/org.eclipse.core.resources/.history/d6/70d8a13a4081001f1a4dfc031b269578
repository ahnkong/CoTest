package level2;

import java.util.Iterator;

public class d241003_숫자의표현 {
	public static void main(String[] args) {
		int answer = 0;
		int n = 15;
		int temp = 0;
		int cnt = 0; 
		
		int midNum = 0;
		if(n % 2 != 0) {
			midNum = n/2 +1;
		}
		System.out.println("셈의 기준이 되는 값 " + midNum);
		
		for (int i = 1; i <= midNum; i++) {
			temp= midNum + (midNum-i*1);
			System.out.println(i + "번째 temp의 값 " + temp);
			if(temp == 15) {				
				cnt++;
				System.out.println("[현재 cnt] : " + cnt);
			}
			System.out.println();
					
		}
	}
}
