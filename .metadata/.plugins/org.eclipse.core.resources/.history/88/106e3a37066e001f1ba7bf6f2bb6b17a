package Level3;

import java.util.Arrays;

// DP : 동적프로그래밍!
// 기억 하기 알고리즘
// 중요! 이전의 결과값을 활용하는 것이다!

//

public class 등굣길 {
	public static void main(String[] args) {
		
	//행렬의 초기값	
	int n = 3; // 행
	int m = 4; // 열

	
	//경로
	int[][] route = new int[3][4];
	
	//웅덩이
	int[][] puddles = {{2,2}};

	// 경로로 셀수 있으면 1이고, 아니면 0으로 둔다
//	route[0][0] = 1;
	
	
	int[][] arr = new int[3][5];
	System.out.println(Arrays.toString(arr));
	
	for(int[] row : arr) {
		System.out.println(row);
	}
	System.out.println(arr.length); // 출력값 : 3
	System.out.println(arr[2].length);  //출력값 : 5 첫번째 행의 열의 길이

	}
	
}
