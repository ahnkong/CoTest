package level0;

import java.util.Arrays;

public class d241030_x만큼간격이_있는_n개의_숫자 {
	public static void main(String[] args) {
		
		int x = 2;
		int n = 5;
		long[] answer = new long[5];
		
		for (int i = 0; i < n; i++) {
			answer[i] = x*(i+1);
		}
		System.out.println(Arrays.toString(answer));
	}
}
