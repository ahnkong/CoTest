package prac;

public class loop_dowhile {
	public static void main(String[] args) {
		
		int sum =0;
		int i = 1;
		//do_while 무조건 한번은 돌려야 할때!
		do {
			sum+=i;
			i++;
			System.out.println("sum : "+ sum);
			
		} while(i<=100); // while조건에 충족하면, 다시 do구문으로 돌아가는거!
		
		
			System.out.println("sum : "+ sum);
			System.out.println("i : "+i);
	}
}
